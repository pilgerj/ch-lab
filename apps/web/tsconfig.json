{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "src",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ],
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "module": "esnext",
    "resolveJsonModule": true
  }
}
